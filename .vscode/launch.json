{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "R-Debugger",
			"request": "launch",
			"name": "Debug R-File - with package",
			"debugMode": "file",
			"workingDirectory": "${workspaceFolder}",
			"file": "example.R",
			"allowGlobalDebugging": true,
			"loadPackages": [ "." ]
		},
		{
			"type": "R-Debugger",
			"request": "launch",
			"name": "Debug testthat test",
			"debugMode": "file",
			"workingDirectory": "${workspaceFolder}",
			"file": "example2.R",
			"allowGlobalDebugging": true,
			"loadPackages": [ "." ],
			"debuggedPackages": [ "temp" ]
		},
		{
			"type": "R-Debugger",
			"request": "launch",
			"name": "Demo splitOverwrittenOutput",
			"debugMode": "file",
			"workingDirectory": "${workspaceFolder}",
			"file": "example3.R",
			"allowGlobalDebugging": true,
			"loadPackages": [ "." ],
			"splitOverwrittenOutput": true,
			"debuggedPackages": [ "temp" ]
		}		
	]
}